case ${TERM} in
    xterm*|rxvt*|Eterm|aterm|kterm|gnome*)
        export TERM="xterm-256color"
        if [ $(tput colors) -ge "256" ]; then
            _theme-powerline
        else
            _theme-simple
        fi
        ;;
    *)
        _theme-simple
        ;;
esac

export BVZSH_THEME='auto'
